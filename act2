a = 5
b = 2
suma = a + b
resta = a - b 
multiplicacion = a * b
division = a / b
modulo = a % b
comparacion_suma_resta = suma > resta

print("Suma:", suma)
print("Resta:", resta)
print("Multiplicacion", multiplicacion)
print("division:", division)
print("Modulo:", modulo)
print("Comparacion de Suma y Resta:", comparacion_suma_resta)
